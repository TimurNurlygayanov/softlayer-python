{% comment %}
  Bootstraps jQuery's CDN and loads local JS files
{% endcomment %}

<script type="text/javascript" src="//code.jquery.com/jquery-1.11.0.min.js"></script>
<script type="text/javascript" src="{{page.baseurl}}public/javascript/modernizr.js"></script>
<script type="text/javascript" src="{{page.baseurl}}public/javascript/main.js"></script>

{% comment %}
